<div class="container">
    <div class="row new-content">
        <h1>コンテンツ作成</h1>
        <button type="button" class="btn btn-outline-secondary"><a href="/homes/index">コンテンツ一覧</a></button>
    </div>
</div>

<div class="container">
    <div class="row">
      <div class="col-12">

        <%= form_with model: @user do |form| %>
        <div class="form-group">
          <%= form.label :name, '名前' %>
          <%= form.text_field :name, class: 'form-control' %>
        </div>
        <div class="form-group">
          <%= form.label :title, 'タイトル' %>
          <%= form.text_field :title, class: 'form-control' %>
        </div>
        <div class="form-group">
          <%= form.label :body, '本文' %>
          <%= form.text_area :body, class: 'form-control', rows: 10 %>
        </div>
        <%= form.submit '保存', class: 'btn btn-primary' %>
      <% end %>
      <div>
  <div>
<div>

